eval "$(starship init zsh)"
eval "$(zoxide init zsh)"
eval "$(sheldon source)"
eval "$(mise activate zsh)"

autoload -U compinit
compinit

# alias
alias ls='eza --icons=auto'
alias cat='bat'
alias vi='nvim'
alias grep='rg'

# Set up fzf key bindings and fuzzy completion
source <(fzf --zsh)

# zeno.zsh
source "$HOME/.config/zsh/zeno.zsh"
